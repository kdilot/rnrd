{
    "compilerOptions": {
        "jsx": "react",
        "target": "es6",
        "module": "commonjs",
        "sourceMap": true,
        "outDir": "dist",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "allowSyntheticDefaultImports": true,
        "baseUrl": ".",
        "paths": {
            "@navigations": ["src/navigations"],
            "@screens": ["src/screens"],
            "@components": ["src/components"]
        }
    },
    "include": ["src/**/*"]
}
